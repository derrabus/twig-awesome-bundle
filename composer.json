{
    "name": "derrabus/twig-awesome-bundle",
    "type": "symfony-bundle",
    "description": "Inline FontAwesome icons into Twig templates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander M. Turek",
            "email": "me@derrabus.de"
        }
    ],
    "require": {
        "php": "^7.1 || ^8",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "fortawesome/font-awesome": "^4.7",
        "psr/log": "^1",
        "symfony/dependency-injection": "^3.3 || ^4 || ^5",
        "symfony/http-kernel": "^3.3 || ^4 || ^5",
        "symfony/polyfill-mbstring": "^1",
        "twig/twig": "^2.4.4 || ^3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^7 || ^8 || ^9",
        "symfony/filesystem": "^3.3 || ^4 || ^5",
        "symfony/framework-bundle": "^3.3 || ^4 | ^5",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/twig-bundle": "^3.4 || ^4 | ^5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Rabus\\TwigAwesomeBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rabus\\TwigAwesomeBundle\\": "tests"
        }
    }
}
