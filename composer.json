{
    "name": "derrabus/twig-awesome-bundle",
    "type": "symfony-bundle",
    "description": "Inline FontAwesome icons into Twig templates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander M. Turek",
            "email": "me@derrabus.de"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "fortawesome/font-awesome": "^5.7",
        "symfony/dependency-injection": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/polyfill-mbstring": "^1",
        "twig/twig": "^2.4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^8",
        "symfony/filesystem": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/twig-bundle": "^4.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Rabus\\TwigAwesomeBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rabus\\TwigAwesomeBundle\\": "tests"
        }
    }
}
