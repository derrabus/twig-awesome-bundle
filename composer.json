{
    "name": "derrabus/twig-awesome-bundle",
    "type": "symfony-bundle",
    "description": "Inline FontAwesome icons into Twig templates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander M. Turek",
            "email": "me@derrabus.de"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "composer-runtime-api": "^2.1",
        "fortawesome/font-awesome": "^6.1",
        "symfony/dependency-injection": "^6",
        "symfony/http-kernel": "^6",
        "twig/twig": "^3.3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9.5.19",
        "symfony/filesystem": "^6",
        "symfony/framework-bundle": "^6",
        "symfony/phpunit-bridge": "^6",
        "symfony/twig-bundle": "^6"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Rabus\\TwigAwesomeBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rabus\\TwigAwesomeBundle\\": "tests"
        }
    }
}
